{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "@vercel/node@2"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/health",
      "destination": "/api/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server.js"
    }
  ]
}
